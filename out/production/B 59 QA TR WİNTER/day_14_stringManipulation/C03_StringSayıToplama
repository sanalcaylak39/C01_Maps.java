package day_14_stringManipulation;

public class C03_SayıToplama {
    public static void main(String[] args) {

        String str1="$13,99";
        String str2="10,55";
        str1=str1.replaceAll("\\D", "");
        str2=str2.replaceAll("\\D","");

        double str1Sayı=Double.valueOf(str1);
        double str2Sayı=Double.valueOf(str2);

        double sonuc=(str1Sayı+str2Sayı)/100;

        System.out.println("verilen Strin sayıların toplamı :$" +sonuc);
    }
    }
}
